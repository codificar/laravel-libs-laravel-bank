    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {  
        "laravel": {
            "providers": [
                "Codificar\\Bank\\BankServiceProvider"
            ],
            "aliases":{}
        }
    },
    "minimum-stability": "dev"
}
