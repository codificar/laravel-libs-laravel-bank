{
    "name": "codificar/bank",
    "description": "A bank library for laravel. Just fork and modify it.",
    "authors": [
        {
            "name": "Vladimir Pereira",
            "email": "vladimir.pereira@codificar.com.br"
        },
        {
            "name": "Raphael Cangucu",
            "email": "raphael.cangucu@codificar.com.br"
        }
    ],
    "repositories": [
        {
          "type": "package",
          "package": {
            "name": "codificar/bank",
            "version": "master",
            "source": {
              "url": "https://libs:ofImhksJ@git.codificar.com.br/laravel-libs/laravel-bank.git",
              "type": "git",
              "reference": "master"
            }
          }
        }
    ], 
    "require": {},
    "autoload": {
        "psr-4": {
            "Codificar\\Bank\\": "src/"
        }
